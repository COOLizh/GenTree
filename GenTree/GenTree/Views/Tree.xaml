<?xml version="1.0" encoding="utf-8" ?>
<ContentPage Title="Tree" Icon="tree.png" 
    xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:xamarinforms="clr-namespace:BottomBar.XamarinForms;assembly=BottomBar.XamarinForms"
             xamarinforms:BottomBarPageExtensions.TabColor="#20b2aa"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:GenTree.ViewModels"
             x:Class="GenTree.Views.Tree">
    <ContentPage.BindingContext>
            <local:MainViewModel/>
        </ContentPage.BindingContext>
    
    <ContentPage.Content>
        <StackLayout VerticalOptions="Center" HorizontalOptions="Center">
            <Label HorizontalOptions="Center" VerticalOptions="Center" FontSize="40" Text="This is tree"/>
        </StackLayout>
    </ContentPage.Content>
    
</ContentPage>